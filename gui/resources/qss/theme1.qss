/*
 * M D B R E A D C R U M B
 */

.MDBreadCrumb {
    background-color: rgba(255, 0, 0, 0.2);
}

.MDBreadCrumb .QPushButton {
    background-color: transparent;
    color: black;
    border-color: blue;
}



/*
 * F I L E   E X P L O R E R   F R A M E
 */

.FileExplorerFrame #top {
    border-bottom-color: transparent;
    background-color: transparent;
}

.FileExplorerFrame #content {
    background-color: pink;
}

.FileExplorerFrame #lvExplorer {
    background-color: transparent;
    border-right-color: gray;
}

.FileExplorerFrame #top #pbDelete,
.FileExplorerFrame #top #pbRename {
    background-color: yellow;
    padding: 12px 0px 12px 16px;
    qproperty-text: "";
    qproperty-iconSize: 24px;
}

.FileExplorerFrame #top #pbDelete:!enabled,
.FileExplorerFrame #top #pbRename:!enabled {
    background-color: lightgray;
}

.FileExplorerFrame #top #pbDelete {
    qproperty-icon: url(:/icons/black-theme/delete_forever);
}

.FileExplorerFrame #top #pbRename {
    qproperty-icon: url(:/icons/black-theme/rename);
}



/*
 * M D T W O L I N E I T E M   ( C O M P A C T )
 */

.MDTwoLineItem {
    background-color: white;
    background-clip: border;
    color: black;
    qproperty-secondaryLabelColor: gray;
}



/*
 * E R R O R   I T E M   W I D G E T
 */

.ErrorItemWidget {
    border-color: green;
    background-color: transparent;
    qproperty-infoIcon: url(:/icons/black-theme/info);
    qproperty-warningIcon: url(:/icons/black-theme/warning);
    qproperty-errorIcon: url(:/icons/black-theme/error);
    qproperty-fatalIcon: url(:/icons/black-theme/critical);
}

.ErrorItemWidget[focusCustom="true"] {
    color: green;
    background-color: yellow;
}
